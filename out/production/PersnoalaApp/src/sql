CREATE TABLE IF NOT EXISTS User (id bigint auto_increment NOT NULL PRIMAR
Y KEY, korisnicko  VARCHAR(255),  lozinka VARCHAR(255));

CREATE TABLE IF NOT EXISTS Ispit (id bigint auto_increment NOT NULL PRIMARY KEY, naziv VARCHAR(255),  ocena int(25), espb int(25));

